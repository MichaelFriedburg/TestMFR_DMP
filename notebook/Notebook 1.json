					"%%pyspark # Verwende PySpark\r\n",
					"\r\n",
					"from pyspark.sql import SparkSession\r\n",
					"from pyspark.sql.types import StructType, StructField, StringType, IntegerType\r\n",
					"\r\n",
					"# Beispiel-Schema f√ºr die Tabelle\r\n",
					"schema = StructType([\r\n",
					"    StructField(\"Name\", StringType(), True),\r\n",
					"    StructField(\"Alter\", IntegerType(), True)\r\n",
					"])\r\n",
					"\r\n",
					"# Beispiel-Daten\r\n",
					"daten = [(\"Anna\", 30),\r\n",
					"         (\"Bob\", 28),\r\n",
					"         (\"Carl\", 35)]\r\n",
					"\r\n",
					"# Spark Session verwenden\r\n",
					"spark = SparkSession.builder.appName(\"SynapseBeispiel\").getOrCreate()\r\n",
					"\r\n",
					"# DataFrame erstellen\r\n",
					"df = spark.createDataFrame(data=daten, schema=schema)\r\n",
					"\r\n",
					"# Zeige die Daten\r\n",
					"df.show()\r\n",
					"\r\n",
					"# Tabelle im Hive Metastore speichern\r\n",
					"df.write.saveAsTable(\"synapse_db.beispiel_tabelle\")\r\n",
					"\r\n",
					"\r\n",
					"\r\n",
					"\r\n",
